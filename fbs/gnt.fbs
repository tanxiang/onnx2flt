namespace nn;

struct versionInfo{
        codeversion:ulong;
        dataversion:ulong;
}

enum DataType:byte { Float, Int, QuantAsymm, QuantSymm, QuantSymmPerChannel}
enum FuseCode:byte { None, Relu, Relu1, Relu6 }

table Tensor {
            name:string(required);
            type:DataType;
            raw_data: [ubyte];
            dim: [ushort];
}

table QuantInfo {
    name: string;
    type: DataType;
    dim: ushort;
    scales: [float32];
    zero_point: int32;
}

table Input {
    name:string;
    dim: [ushort];
}

table CONV_2D {
    input: string(required);
    output: string(required);
    name: string;
    weight: string;
    bias: string;
    padding_left: int;
    padding_right: int;
    padding_top: int;
    padding_bottom: int;
    stride_x: int;
    stride_y: int;
    fuse_code: FuseCode;
    nchw: bool;
    dilation_x: int;
    dilation_y: int;

}

table AVERAGE_POOL_2D {
    input:string(required);
    output: string(required);
    name: string;
    padding_left: int;
    padding_right: int;
    padding_top: int;
    padding_bottom: int;
    stride_x: int;
    stride_y: int;
    kernel_width: int;
    kernel_height: int;
    fuse_code: FuseCode;
}


table MAX_POOL_2D {
    input: string(required);
    output: string(required);
    name: string;
    padding_left: int;
    padding_right: int;
    padding_top: int;
    padding_bottom: int;
    stride_x: int;
    stride_y: int;
    kernel_width: int;
    kernel_height: int;
    fuse_code: FuseCode;
}

table RELU {
    input: string(required);
    output: string(required);
    name: string;
}

table SOFTMAX {
    input: string(required);
    output: string(required);
    name: string;
    beta: float;
}

table FULLY_CONNECTED {
    input:string(required);
    output: string(required);
    name: string;
    weight: string;
    bias: string;
    fuse_code: FuseCode;
}

table ADD {
    input:  [string](required);
    output: string(required);
    name: string;
    fuse_code: FuseCode;
}


table CONCATENATION {
    input: [string](required);
    output: string(required);
    name: string;
    axis: int;
}

table DEPTHWISE_CONV_2D {
    input: string(required);
    output: string(required);
    name: string;
    weight: string;
    bias: string;
    padding_left: int;
    padding_right: int;
    padding_top: int;
    padding_bottom: int;
    stride_x: int;
    stride_y: int;
    depth_multiplier: int;
    fuse_code: FuseCode;
}

table BATCH_TO_SPACE_ND {
    input: string(required);
    output: string(required);
    name: string;
    block_sizes: [int];
}

table SPACE_TO_BATCH_ND {
    input:  string(required);
    output: string(required);
    name: string;
    block_sizes: [int];
    pads: [int];
}

table STRIDED_SLICE {
    input: string(required);
    output: string(required);
    name: string;
    starts: [int];
    ends: [int];
    strides: [int];
    begin_mask: int;
    end_mask: int;
    shrink_axis_mask: int;
}

table MUL {
    input:[string](required);
    output: string(required);
    name: string;
    fuse_code: FuseCode;
}

table DEQUANTIZE {
    input: string(required);
    output: string(required);
    name: string;
}

table LOCAL_RESPONSE_NORMALIZATION {
    input:string(required);
    output: string(required);
    name: string;
    radius: int;
    bias: float;
    alpha: float;
    beta: float;
}

table TANH {
    input: string(required);
    output: string(required);
    name: string;
}

table FLOOR {
    input: string(required);
    output: string(required);
    name: string;
}

table LOGISTIC {
    input: string(required);
    output: string(required);
    name: string;
}

table PRELU {
    input: string(required);
    output: string(required);
    name: string;
    alpha: string;
}

table POW {
    input: string(required);
    output: string(required);
    name: string;
    exp: string;
}

table NEG {
    input: string(required);
    output: string(required);
    name: string;
}

table MINIMUM {
    input: [string](required);
    output: string(required);
    name: string;
}

table MAXIMUM {
    input: [string](required);
    output: string(required);
    name: string;
}

table LOG {
    input: string(required);
    output: string(required);
    name: string;
}

table ABS {
    input: string(required);
    output: string(required);
    name: string;
}

table EXP {
    input: string(required);
    output: string(required);
    name: string;
}

table SUB {
    input: [string](required);
    output: string(required);
    name: string;
    fuse_code: FuseCode;
}

union Layer { 
CONV_2D,AVERAGE_POOL_2D,MAX_POOL_2D,RELU,SOFTMAX,FULLY_CONNECTED,
ADD,CONCATENATION,DEPTHWISE_CONV_2D,BATCH_TO_SPACE_ND,SPACE_TO_BATCH_ND,
STRIDED_SLICE,MUL,DEQUANTIZE,LOCAL_RESPONSE_NORMALIZATION,TANH,FLOOR,LOGISTIC,
PRELU,POW,NEG,MINIMUM,MAXIMUM,LOG,ABS,EXP,SUB
}

table Graph {
        version:versionInfo;
        node:[Layer];
    initializers:[Tensor];
    inputs:[Input];
    quant_infos:[QuantInfo];
    outputs:[string];
}

root_type Graph;
